{
  "name": "@travi/eslint-config-travi",
  "version": "0.0.0-semantically-released",
  "description": "my eslint config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/travi/eslint-config-travi.git"
  },
  "keywords": [
    "eslint",
    "config",
    "eslintconfig",
    "es6",
    "react",
    "mocha"
  ],
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/travi/eslint-config-travi/issues"
  },
  "homepage": "https://github.com/travi/eslint-config-travi#readme",
  "scripts": {
    "test": "eslint rules",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "dependencies": {
    "eslint": "4.5.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-cucumber": "travi/eslint-plugin-cucumber#callback",
    "eslint-plugin-eslint-comments": "1.0.3",
    "eslint-plugin-filenames": "1.2.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-mocha": "4.11.0",
    "eslint-plugin-react": "7.3.0",
    "eslint-plugin-security": "1.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "3.1.2",
    "cz-conventional-changelog": "2.0.0",
    "husky": "0.14.3",
    "semantic-release": "7.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
